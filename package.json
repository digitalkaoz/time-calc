{
  "name": "time-calc",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-require-ignore": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-app": "^2.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.2.0",
    "critical": "^0.8.4",
    "enzyme": "^2.7.1",
    "enzyme-redux": "^0.1.6",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "fetch-mock": "^5.9.4",
    "inline-critical": "^2.4.0",
    "jest-enzyme": "^3.0.1",
    "jsdom": "^10.0.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "^0.9",
    "redux-test-utils": "^0.1.2",
    "standard": "^10.0.2",
    "sw-precache": "^5.1.1",
    "why-did-you-update": "^0.0.8"
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.7",
    "draggabilly": "digitalkaoz/draggabilly#patch-1",
    "get-size": "digitalkaoz/get-size#patch-1",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^3.7.3",
    "material-design-lite": "digitalkaoz/material-design-lite#feature/ssr-capability",
    "md-date-time-picker": "puranjayjain/md-date-time-picker#master",
    "moment": "^2.17.1",
    "moment-duration-format": "https://github.com/jsmreese/moment-duration-format/archive/dev.tar.gz",
    "react": "^15.4.2",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.4.2",
    "react-forms": "beta",
    "react-redux": "^5.0.2",
    "recompose": "^0.23.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.0",
    "store": "^2.0.4",
    "unidragger": "digitalkaoz/unidragger#patch-1",
    "unipointer": "digitalkaoz/unipointer#patch-1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "dev": "concurrently \"cd server && npm run dev\" \"npm run client:start\" \"npm run watch-css\"",
    "start": "npm run client:build && npm start --prefix server",
    "client:start": "react-scripts start",
    "client:build": "rm -rf build/* && mv .env ._env && npm run build-css && react-scripts build && npm run server:build && mv ._env .env",
    "client:test": "react-scripts test --env=jsdom",
    "client:coverage": "npm run client:test -- --coverage",
    "client:eject": "react-scripts eject",
    "server:build": "npm run ssr && npm run sw",
    "deploy": "cd server && npm run deploy",
    "sw": "sw-precache --config=scripts/sw-precache-config.js",
    "cs": "standard --fix src/**/*.js src/**/**/*.js server/src/**/*.js -v --color --parser babel-eslint --env jest --env browser --env node --env serviceWorker",
    "ssr": "BABEL_ENV=production node_modules/.bin/babel-node scripts/ssr.js"
  }
}
