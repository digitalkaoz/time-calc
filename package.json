{
  "name": "time-calc",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.2.0",
    "critical": "^1.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-redux": "^0.1.6",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "fetch-mock": "^5.10.0",
    "inline-critical": "^2.4.0",
    "jest-enzyme": "^4.0.1",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "^1.0.17",
    "react-snapshot": "^1.3.0",
    "redux-test-utils": "^0.2.1",
    "standard": "^10.0.1",
    "sw-precache": "^5.0.0",
    "why-did-you-update": "^0.1.0"
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.7",
    "draggabilly": "digitalkaoz/draggabilly#patch-1",
    "get-size": "digitalkaoz/get-size#patch-1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "json2csv": "^3.7.3",
    "material-design-lite": "digitalkaoz/material-design-lite#feature/ssr-capability",
    "md-date-time-picker": "puranjayjain/md-date-time-picker#master",
    "moment": "^2.17.1",
    "moment-duration-format": "https://github.com/jsmreese/moment-duration-format/archive/dev.tar.gz",
    "prop-types": "^15.5.8",
    "react": "^16.1.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.1.1",
    "react-forms": "jducro/react-forms#fix-react-warnings",
    "react-redux": "^5.0.4",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.16.0",
    "store": "^2.0.4",
    "unidragger": "digitalkaoz/unidragger#patch-1",
    "unipointer": "digitalkaoz/unipointer#patch-1"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "dev": "concurrently \"cd server && npm run dev\" \"npm run client:start\" \"npm run watch-css\"",
    "start": "npm run client:build && npm run server:build && npm start --prefix server",
    "critical": "cat ./build/index.html | critical --base=./build --folder=./build --inline --minify > ./build/index_critical.html && mv ./build/index_critical.html ./build/index.html",
    "postinstall": "cd node_modules/react-forms && npm i && make build && cd - && rm -rf node_modules/draggabilly/node_modules && rm -rf node_modules/unidragger/node_modules",
    "client:start": "react-scripts start",
    "client:build": "rm -rf build/* && mv .env ._env && npm run build-css && react-scripts build && mv ._env .env",
    "client:test": "react-scripts test --env=jsdom",
    "client:coverage": "npm run client:test -- --coverage",
    "client:eject": "react-scripts eject",
    "server:build": "npm run ssr && npm run sw",
    "deploy": "cd server && npm run deploy",
    "sw": "sw-precache --config=scripts/sw-precache-config.js",
    "cs": "standard --fix src/**/*.js src/**/**/*.js server/src/**/*.js -v --color --parser babel-eslint --env jest --env browser --env node --env serviceWorker",
    "ssr": "react-snapshot && BABEL_ENV=production node_modules/.bin/babel-node scripts/ssr.js"
  }
}
